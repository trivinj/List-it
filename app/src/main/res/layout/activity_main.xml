<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="87dp"
        android:layout_height="88dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/stats"
        android:layout_marginEnd="83dp"
        app:srcCompat="@android:drawable/ic_menu_today"
        android:layout_alignParentRight="true"
        android:layout_marginRight="83dp" />

    <ImageButton
        android:id="@+id/stats"
        android:layout_width="87dp"
        android:layout_height="88dp"
        android:layout_alignLeft="@+id/events"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/events"
        android:layout_marginBottom="158dp"
        app:srcCompat="@drawable/stat" />

    <ImageButton
        android:id="@+id/events"
        android:layout_width="87dp"
        android:layout_height="88dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/todo"
        android:layout_marginEnd="82dp"
        android:layout_marginRight="82dp"
        app:srcCompat="@drawable/calender" />

    <ImageButton
        android:id="@+id/notes"
        android:layout_width="87dp"
        android:layout_height="88dp"
        android:layout_alignLeft="@+id/todo"
        android:layout_alignStart="@+id/todo"
        android:layout_alignTop="@+id/imageButton5"
        app:srcCompat="@drawable/note" />

    <ImageButton
        android:id="@+id/todo"
        android:layout_width="87dp"
        android:layout_height="88dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="84dp"
        android:layout_marginTop="131dp"
        app:srcCompat="@drawable/todo" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView7"
        android:layout_alignTop="@+id/textView8"
        android:text="Statistics"
        android:layout_alignRight="@+id/textView7" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/textView6"
        android:layout_marginBottom="126dp"
        android:text="Notes"
        android:layout_alignLeft="@+id/textView6" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/textView6"
        android:layout_marginEnd="99dp"
        android:text="Events"
        android:layout_alignParentRight="true"
        android:layout_marginRight="99dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="112dp"
        android:layout_marginTop="227dp"
        android:text="ToDo"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="112dp" />
</RelativeLayout>